# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=huber_db
DB_USERNAME=postgres
DB_PASSWORD=password
# Optional: set to 'require' for cloud providers like Supabase/Neon
DB_SSLMODE=require

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
TOGETHER_API_KEY=your_together_api_key_here

# Langfuse Configuration
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key_here
LANGFUSE_HOST=https://cloud.langfuse.com

# Weights & Biases Configuration
WANDB_API_KEY=your_wandb_api_key_here
WANDB_ENTITY=your_wandb_entity_or_username
WANDB_PROJECT=prompt-evaluation

# Admin Configuration
ADMIN_PASSWORD=your_admin_password_here
SECRET_KEY=your_secret_key_here

# Evaluation Settings
QA_PAIRS_PATH=qa_pairs_filtered.csv
DEFAULT_PROMPT_TYPE=advanced 
# Model Configuration for testing
# Use text-embedding-3-small (1536 dims) to match database schema
# text-embedding-3-large (3072 dims) requires schema migration
EMBEDDING_MODEL=text-embedding-3-small
RERANKER_MODEL='cross-encoder/ms-marco-MiniLM-L-6-v2' 